{
  "publisher": "niko-ni",
  "name": "vscode-ext-qrcode",
  "displayName": "vscode-ext-qrcode",
  "version": "0.3.15",
  "packageManager": "pnpm@8.3.1",
  "description": "The vscode extension for v-qr-code-next module.",
  "author": "Niko Ni",
  "license": "MIT",
  "homepage": "https://github.com/bluepower/vscode-ext-qrcode#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluepower/vscode-ext-qrcode"
  },
  "bugs": {
    "url": "https://github.com/bluepower/vscode-ext-qrcode/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "public/icon.png",
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "vscode": "^1.77.3"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "vpoc.qrcode",
        "title": "Vite POC - QR Code",
        "category": "v-poc-ext"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "pnpm run build --watch",
    "vscode:prepublish": "pnpm run build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@types/vscode": "^1.77.0",
    "@vscode/vsce": "^2.19.0",
    "bumpp": "^9.1.0",
    "pnpm": "^8.3.1",
    "rimraf": "^5.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "v-qr-code-next": "^0.2.3"
  }
}
